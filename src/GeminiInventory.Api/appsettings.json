{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "GeminiInventoryDbContext": "Host=localhost;Database=gemini_inventory_db;Username=postgres;Password=yourpassword"
  },
  "AWS": {
    "UseLocalStack": true,
    "LocalStack": {
      "ServiceUrl": "http://localhost:4566"
    },
    "Region": "us-east-1",
    "Profile": "default",
    "EventBridge": {
      "EventBusName": "gemini",
      "Source": "gemini"
    }
  },
  "QueueSettings": {
    "OrderSubmitted": "http://sqs.us-east-1.localhost.localstack.cloud:4566/000000000000/order-submitted.fifo"
  },
  "AllowedHosts": "*"
}